{
  "name": "@avonjs/avonjs",
  "version": "1.2.1",
  "description": "A fluent Node.js API generator.",
  "main": "dist/index.js",
  "types": "dist/index.ts",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --runInBand",
    "test:debug": "jest --runInBand --verbose"
  },
  "bin": {
    "avonjs": "./dist/cli.js"
  },
  "keywords": [
    "express",
    "avon",
    "avonjs"
  ],
  "author": "zarehesmaiel@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.8",
    "@types/node": "^18.13.0",
    "@types/pluralize": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "body-parser": "^1.20.2",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "openapi-types": "^12.1.3",
    "prettier": "2.8.4",
    "supertest": "^6.3.3",
    "typescript": "^5.2.2"
  },
  "importSort": {
    ".js, .jsx, .es6, .es, .ts, .tsx": {
      "parser": "typescript",
      "style": "eslint",
      "options": {}
    }
  },
  "dependencies": {
    "collect.js": "^4.36.1",
    "commander": "^11.1.0",
    "express-jwt": "^8.4.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "moment": "^2.29.4",
    "pluralize": "^8.0.0"
  }
}
